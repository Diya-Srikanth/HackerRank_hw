#1 Polar Coordinates

import cmath
a = input()
r = abs(complex(a))
rho = cmath.phase(complex(a))
print(r)
print(rho)
     
     ------------------------------------
     
#2 Find Angle MBC

import math
a=int(input())
b=int(input())
s=int(round(math.degrees(math.atan2(b,a))))
print(str(180-(90+s))+"Â°")
    
